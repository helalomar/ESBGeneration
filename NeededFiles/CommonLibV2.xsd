	<xsd:complexType name="ContactMngLstItem_Type">
		<xsd:sequence>
			<xsd:element name="ContactMngFunction" type="arb:RequestType_Type" />
			<xsd:element name="ContactType" type="arb:ContactType_Type" />
			<xsd:element name="ContactVal" type="arb:Contact_Type"
				minOccurs="0" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ErrorLst_Type">
		<xsd:sequence>
			<xsd:element name="ErrorLstItem" type="arb:ErrorLstItem_Type" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ErrorLstItem_Type">
		<xsd:sequence>
			<xsd:element name="ErrorStatusCd" type="arb:ErrorStatusCode_Type"/>
			<xsd:element name="ErrorStatusDesc" type="arb:Desc400_Type" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="String_Type">
		<xsd:restriction base="xsd:string" />
	</xsd:simpleType>
	<xsd:simpleType name="Double_Type">
		<xsd:restriction base="xsd:double" />
	</xsd:simpleType>
	<xsd:simpleType name="Integer_Type">
		<xsd:restriction base="xsd:integer" />
	</xsd:simpleType>
	<xsd:simpleType name="Short_Type">
		<xsd:restriction base="xsd:short" />
	</xsd:simpleType>
	<xsd:simpleType name="Long_Type">
		<xsd:restriction base="xsd:long" />
	</xsd:simpleType>
	<xsd:simpleType name="Decimal_Type">
		<xsd:restriction base="xsd:decimal" />
	</xsd:simpleType>
	<xsd:simpleType name="Time_Type">
		<xsd:restriction base="xsd:time">
			<xsd:pattern value="(([01][0-9])|(2[0-3]))(:[0-5][0-9]){2}(\.[0-9]{1,3})?" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ShortTime_Type">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{1,2}:\d{1,2}(:\d{1,2})? (AM|PM)" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Timestamp_Type">
		<xsd:restriction base="arb:String_Type">
			<xsd:pattern
				value="(\p{Nd}{4}-\p{Nd}{2}-\p{Nd}{2}T\p{Nd}{2}:\p{Nd}{2}:\p{Nd}{2})|(\p{Nd}{4}-\p{Nd}{2}-\p{Nd}{2}T\p{Nd}{2}:\p{Nd}{2}:\p{Nd}{2}.\p{Nd}{3}Z)" />
			<!-- 2014-10-30T15:12:35 OR 2014-10-30T15:12:35.000Z -->
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Date_Type">
		<xsd:restriction base="xsd:date">
			<xsd:pattern value="\p{Nd}{4}-\p{Nd}{2}-\p{Nd}{2}" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="HijriDate_Type">
		<xsd:restriction base="arb:String_Type">
			<xsd:pattern value="\p{Nd}{4}-\p{Nd}{2}-\p{Nd}{2}" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Year_Type">
		<xsd:restriction base="arb:String_Type">
			<xsd:maxLength value="4" />
			<xsd:pattern value="(1|2)\p{Nd}{3}" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Month_Type">
		<xsd:restriction base="arb:LookupID_Type">
			<xsd:enumeration value="01" />
			<xsd:enumeration value="02" />
			<xsd:enumeration value="03" />
			<xsd:enumeration value="04" />
			<xsd:enumeration value="05" />
			<xsd:enumeration value="06" />
			<xsd:enumeration value="07" />
			<xsd:enumeration value="08" />
			<xsd:enumeration value="09" />
			<xsd:enumeration value="10" />
			<xsd:enumeration value="11" />
			<xsd:enumeration value="12" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Binary_Type">
		<xsd:restriction base="xsd:base64Binary" />
	</xsd:simpleType>
	<xsd:simpleType name="Flg_Type">
		<xsd:restriction base="arb:LookupID_Type">
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="YNFlg_Type">
		<xsd:annotation>
			<xsd:documentation>
				Y: Yes
				N: No
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="arb:Flg_Type">
			<xsd:enumeration value="Y" />
			<xsd:enumeration value="N" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="UDSXFlg_Type">
	
		<xsd:restriction base="arb:Flg_Type">
			<!--<xsd:enumeration value="U" />
			<xsd:enumeration value="D" />
			<xsd:enumeration value="S" />
			<xsd:enumeration value="X" />
			‘U’ : Upgrade ‘D’ : Downgrade ‘S’ : Same 'X' : Not Available/ Not 
			Used -->
		</xsd:restriction>
		
	</xsd:simpleType>
	<xsd:simpleType name="TFFlg_Type">
		<xsd:annotation>
			<xsd:documentation>
				T: True
				F: False
			</xsd:documentation>
		</xsd:annotation>
		
		<xsd:restriction base="arb:Flg_Type">
			<!--<xsd:enumeration value="T" />
			<xsd:enumeration value="F" />-->
		</xsd:restriction>
		
	</xsd:simpleType>
	<xsd:simpleType name="HGFlg_Type">
		<xsd:annotation>
			<xsd:documentation>
				H: Hijri
				G: Gregorian
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="arb:Flg_Type">
			<xsd:enumeration value="H" />
			<xsd:enumeration value="G" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ID_Type">
