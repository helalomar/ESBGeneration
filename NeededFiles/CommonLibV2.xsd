	<xsd:complexType name="MsgRqHdr_Type">
		<xsd:annotation>
			<xsd:documentation>Common Request Message Header</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Msg" type="arb:Msg_Type" />
			<xsd:element name="Agt" type="arb:Agt_Type" minOccurs="0" />
			<xsd:element name="Sys" type="arb:Sys_Type" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="MsgRsHdr_Type">
		<xsd:annotation>
			<xsd:documentation>Common Response Message Header</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Status" type="arb:Status_Type" />
			<xsd:element name="RqID" type="arb:RqID_Type" minOccurs="0" />
			<xsd:element name="AckFlg" type="arb:YNFlg_Type"
				minOccurs="0" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Msg_Type">
		<xsd:sequence>
			<xsd:element name="RqID" type="arb:RqID_Type" />
