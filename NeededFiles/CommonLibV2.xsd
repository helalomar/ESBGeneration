	</xsd:complexType>
	<xsd:complexType name="LoginInfo_Type">
		<xsd:sequence>
			<xsd:element name="TerminalID" type="arb:TerminalID20_Type"
				minOccurs="0" />
			<xsd:element name="TerminalDesc" type="arb:TerminalDesc_Type"
				minOccurs="0" />
			<xsd:element name="IPAddr" type="arb:IPAddr_Type"
				minOccurs="0" />
			<xsd:element name="UserID" type="arb:UserID_Type"
				minOccurs="0" />
			<xsd:element name="CardNo" type="arb:CardNum28_Type"
				minOccurs="0" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Status_Type">
		<xsd:annotation>
			<xsd:documentation>Status</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="StatusCd" type="arb:StatusCode_Type"><xsd:annotation><xsd:documentation xml:lang="en">Status Code</xsd:documentation></xsd:annotation></xsd:element>
			<xsd:element name="StatusDesc" type="arb:Desc400_Type" minOccurs="0"><xsd:annotation><xsd:documentation xml:lang="en">Status Description</xsd:documentation></xsd:annotation></xsd:element>
			<xsd:element name="RtnCd" type="arb:RtnCode_Type" minOccurs="0"><xsd:annotation><xsd:documentation xml:lang="en">Return Code</xsd:documentation></xsd:annotation></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RecPgCtrlIn_Type">
		<xsd:annotation>
			<xsd:documentation>Record Pagination Control Input
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="MaxRecs" type="arb:MaxRecs_Type" />
			<xsd:element name="Offset" type="arb:Offset_Type" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RecPgCtrlOut_Type">
		<xsd:annotation>
			<xsd:documentation>Record Pagination Control Output
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SentRecs" type="arb:SentRecs_Type" />
			<xsd:element name="MatchedRecs" type="arb:MatchedRecs_Type" />
			<xsd:element name="ComplFlg" type="arb:YNFlg_Type" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Amt_Type">
		<xsd:annotation>
			<xsd:documentation>Amount</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AmtVal" type="arb:AmtVal_Type" />
			<xsd:element name="DecPt" type="arb:DecPt_Type" />
			<xsd:element name="Sgn" type="arb:Sgn_Type" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BigAmt_Type">
		<xsd:annotation>
			<xsd:documentation>Amount value with length up to 17 digit used for
				Sum Amounts</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AmtVal" type="arb:BigAmtVal_Type" />
			<xsd:element name="DecPt" type="arb:DecPt_Type" />
			<xsd:element name="Sgn" type="arb:Sgn_Type" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Acct_Type">
		<xsd:annotation>
			<xsd:documentation>Account</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AcctBranch" type="arb:AcctBranch_Type" />
			<xsd:element name="AcctTyp" type="arb:AcctType3_Type" />
			<xsd:element name="AcctNum" type="arb:AcctNum13_Type" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardID_Type">
		<xsd:sequence>
			<xsd:element name="CardNum" type="arb:CardNum28_Type" />
			<xsd:element name="CardTyp" type="arb:CardType_Type"
				minOccurs="0" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SysSbscrptnID_Type">
		<xsd:sequence>
			<xsd:element name="SysSbscrptnNum" type="arb:TrxnRef36_Type" />
			<xsd:element name="SysSbscrptnTyp" type="arb:SysSbscrptnType_Type" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Sec_Type">
		<xsd:sequence>
			<xsd:element name="Info" type="arb:Desc_Type" />
			<xsd:element name="InfoTyp" type="arb:SecInfoType_Type" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Override_Type">
		<xsd:sequence>
			<xsd:element name="UserID" type="arb:UserID_Type" />
			<xsd:element name="Sec" type="arb:Sec_Type" minOccurs="0" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CurrencyAmt_Type">
		<xsd:sequence>
			<xsd:element name="Amount" type="arb:Decimal_Type" />
			<xsd:element name="Currency" type="arb:Currency_Type" />
		</xsd:sequence>
		<!-- Amount and Currency in ISO format -->
	</xsd:complexType>
	<xsd:complexType name="ActivityLst_Type">
		<xsd:sequence>
			<xsd:element name="ActivityLstItem" type="arb:ActivityLstItem_Type"
				maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ActivityLstItem_Type">
		<xsd:sequence>
			<xsd:element name="ActivityDate" type="arb:Date_Type" />
			<xsd:element name="ActivityTime" type="arb:Time_Type" />
			<xsd:element name="ActivityDtls" type="arb:Desc100_Type" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ContactLst_Type">
		<xsd:sequence>
			<xsd:element name="ContactLstItem" type="arb:ContactLstItem_Type"
				maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ContactLstItem_Type">
		<xsd:sequence>
			<xsd:element name="ContactType" type="arb:ContactType_Type" />
			<xsd:element name="ContactRecepFlg" type="arb:ContactRecepFlg_Type"
				minOccurs="0" />
			<xsd:element name="ContactVal" type="arb:Contact_Type" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ContactMngLst_Type">
		<xsd:sequence>
			<xsd:element name="ContactMngLstItem" type="arb:ContactMngLstItem_Type"
				maxOccurs="unbounded" />
		</xsd:sequence>
